---
import BaseLayout from "../../layouts/BaseLayout.astro";
import {uniqueTags} from "./[tag].astro";
---

<BaseLayout pageTitle="Tags">
    <div class="tags">
        {uniqueTags.map((tag: string) => <p class="tag">
            <a href={`/tags/${tag}`}>
                {tag}
            </a>
        </p>)}
    </div>
</BaseLayout>